{
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				}
			]
		}
	],
	"rewrites": [
		{
			"source": "/:path*",
			"has": [
				{
					"type": "host",
					"value": "(?<subdomain>[^.]+).spaark.dev"
				}
			],
			"destination": "/:subdomain/:path*"
		}
	],
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/next"
		}
	],
	"buildCommand": "prisma generate && next build",
	"installCommand": "npm install"
}
